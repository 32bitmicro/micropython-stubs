{
    "include": [
        "."
    ],
    "typeshedPaths": [
        "./typings"
    ],
    "exclude": [
        ".*",
        "__*",
        "**/typings"
    ],
    "ignore": [
        "**/typings"
    ],
    // "pythonVersion": "3.8",
    "pythonPlatform": "Linux",
    "verboseOutput": false,
    "typeCheckingMode": "basic",
    // "typeshedPath": "./typings",
    "reportMissingModuleSource": "none",
    // Used in stdlib check to verify that CPython module variables are not available
    // such as:  sys.executable    
    "reportUnnecessaryTypeIgnoreComment" : "error"
}